<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor
    configVersion="3"
    xmlns="https://www.phpdoc.org"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://www.phpdoc.org https://raw.githubusercontent.com/phpDocumentor/phpDocumentor/master/data/xsd/phpdoc.xsd"
>
    <paths>
        <output>../../documentation/generated</output>
        <cache>../../documentation/cache</cache>
    </paths>
    <version number="1.0.0">
        <api>
            <source dsn="../..">
                <path>site</path>
            </source>
            <output>api</output>
            <ignore>
                <path>vendor</path>
                <path>tests</path>
                <path>database</path>
            </ignore>
            <extensions>
                <extension>php</extension>
            </extensions>
            <markers>
                <marker>TODO</marker>
                <marker>FIXME</marker>
            </markers>
        </api>
    </version>
    <title>LAMP Start Project Documentation</title>
    <template name="default"/>
</phpdocumentor>
